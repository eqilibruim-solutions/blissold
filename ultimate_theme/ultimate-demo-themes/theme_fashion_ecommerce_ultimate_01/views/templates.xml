<odoo>
    <data>
        <!--Snippet Banner Start-->
        <template id="fashion_banner" name="Fashion Banner">
            <section
                    class="s_cover parallax align-content-center align-items-center d-flex full-height  s_parallax_no_overflow_hidden"
                    data-scroll-background-ratio="0" style="background-image: none;">
                <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                      style="background-image: url(&quot;/theme_fashion_ecommerce_ultimate_01/static/src/img/placeholder-image-1170.jpg&quot;); background-position: 50% 0px;"/>
                <div class="container">
                    <div class="row s_nb_column_fixed">
                        <div class="col-lg-12 s_text" data-name="Text">
                            <p class="lead text-left s_letter_spacing_2" style="text-align: center;">
                                <font style="color: rgb(255, 255, 255);">WINTER 2019</font>
                            </p>
                        </div>
                        <div class="col-lg-12 s_title" data-name="Title">
                            <h1 class="text-left s_letter_spacing_2">
                                <font style="color: rgb(255, 255, 255); font-size: 38px;">PRINTED SWEATSHIRT</font>
                                <br/>
                                <font style="color: rgb(255, 255, 255); font-size: 38px;">WITH HOODED</font>
                            </h1>
                        </div>
                        <div class="col-lg-12 pt16 pb16" data-name="Buttons">
                            <a href="/contactus" class="mt8 mb8 btn btn-outline-gamma" data-original-title="" title=""
                               aria-describedby="tooltip447539">EXPLORE NOW
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Banner End-->

        <!--Snippet Image Block Start-->
