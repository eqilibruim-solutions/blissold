<odoo>
    <data>

        <!--Snippet  Promo Image Start-->
        <template id="interior_promo_image" name="Interior Promo Image">
            <section class="pt64 pb64" id="ecommerce_block_1">
                <div class="container">
                    <div class="row">
                        <div class="col-12 col-md-6 col-lg-6 col-sm-6 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto" src="/theme_interior_ecommerce_ultimate_01/static/src/img/placeholder-600.jpg"
                                     data-original-title="" title="" aria-describedby="tooltip867057"/>
                                <div class="col-12 img-content px-5">
                                    <h2>
                                        <b>
                                            <font style="" class="text-beta">Choose Your</font>
                                        </b>
                                    </h2>
                                    <h2>
                                        <b>
                                            <font style="" class="text-beta">Best Collection</font>
                                        </b>
                                    </h2>
                                    <p>
                                        <a href="#" data-original-title="" title="" aria-describedby="tooltip177010"
                                           class="btn btn-beta flat">
                                            <b>Shop Now</b>
                                        </a>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-6 col-lg-6 col-sm-6 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto" src="/theme_interior_ecommerce_ultimate_01/static/src/img/placeholder-600.jpg"
                                     data-original-title="" title="" aria-describedby="tooltip867057"/>
                                <div class="col-12 img-content px-5">
                                    <h2>
                                        <b>
                                            <font style="" class="text-beta">20% Off</font>
                                        </b>
                                    </h2>
                                    <h2>
                                        <b>
                                            <font style="" class="text-beta">On New Collection</font>
                                        </b>
                                    </h2>
                                    <p>
                                        <a href="#" class="btn btn-beta flat">
                                            <b>Shop Now</b>
                                        </a>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet  Promo Image End-->

        <!--Snippet  Deal Image Start-->
        <template id="interior_deal_image" name="Interior Deal Image">
            <section class="pt64 pb64 oe_img_bg oe_custom_bg"
                     style="background-image: url(&quot;/theme_interior_ecommerce_ultimate_01/static/src/img/placeholder-1170.jpg&quot;);">
                <div class="container">
                    <div class="row justify-content-end">
                        <div class="col-12 col-sm-12 col-md-6 col-lg-6 right bg-light text-center pt64 pb64">
                            <h2>
                                <b>
                                    <font>Deal Of The Day</font>
                                </b>
                            </h2>
                            <h1 class="mb32">10% DISCOUNT</h1>
                            <a href="#" class="text-decoration-none" data-original-title="" title=""
                               aria-describedby="tooltip250793">View More
                                <i class="fa fa-chevron-right"/>
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet  Deal Image End-->

        <!--Snippet  Sale Image Start-->
        <template id="interior_sale_image" name="Interior Sale Image">
            <section class="pt64 pb64" id="ecommerce_block_2">
                <div class="container">
                    <div class="row">
                        <div class="col-12 co-md-12 col-lg-12 col-sm-12 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto"
                                     src="/theme_interior_ecommerce_ultimate_01/static/src/img/placeholder-1170x400.png"
                                     data-original-title="" title="" aria-describedby="tooltip813211"/>
                                <div class="img-content-1 col-12 text-center">
                                    <h3>
                                        <font style="color: rgb(255, 255, 255);">
                                            <b>Get Best Interior</b>
                                        </font>
                                    </h3>
                                    <div class="text-center" data-name="Buttons">
                                        <a href="#" class="mt8 mb8 btn btn-alpha rounded-circle" data-original-title=""
                                           title="" aria-describedby="tooltip458703">View More
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet  Sale Image End-->

        <!--Snippet  Blog Start-->
        <template id="interior_blog" name="Interior Blog">
            <section class="s_blog_carousel js_get_objects blog_carousel s_headline_center pt48" data-loading="true"
                     data-template="snippet_blog_carousel_73lines.blog_list_template" data-object_name="blog.post"
                     style=""/>
        </template>
        <!--Snippet Blog End-->

        <!--Snippets Assets-->
        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Interior Ecommerce Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_interior_ecommerce_ultimate_01.interior_promo_image"
                           t-thumbnail="/theme_interior_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_interior_ecommerce_ultimate_01.interior_deal_image"
                           t-thumbnail="/theme_interior_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_interior_ecommerce_ultimate_01.interior_sale_image"
                           t-thumbnail="/theme_interior_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_interior_ecommerce_ultimate_01.interior_blog"
                           t-thumbnail="/theme_interior_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>

                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>