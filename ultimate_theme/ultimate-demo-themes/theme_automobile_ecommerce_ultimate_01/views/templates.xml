<odoo>
    <data>
        <!--Snippet Banner Start-->
        <template id="automobile_banner" name="Automobile Banner">
            <section
                    class="s_cover parallax align-content-center align-items-center d-flex full-height s_parallax_is_fixed s_parallax_no_overflow_hidden"
                    data-scroll-background-ratio="1" style="background-image: none;">
                <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                      style="background-image: url(&quot;/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-1170.jpg&quot;); background-position: 50% 0px;"/>
                <div class="container">
                    <div class="row s_nb_column_fixed">
                        <div class="col-lg-12 s_title" data-name="Title">
                            <h1 style="font-size: 62px; text-align: center;">
                                <font style="background-color: rgba(2, 2, 2, 0.34); color: rgb(255, 255, 255); font-size: 24px;"
                                      class="s_letter_spacing_3">HIGH-END
                                </font>
                            </h1>
                            <h1 style="font-size: 62px; text-align: center;">
                                <font style="background-color: rgba(2, 2, 2, 0.34); color: rgb(255, 255, 255);">New Car
                                    Interior
                                </font>
                            </h1>
                        </div>
                        <div class="col-lg-12 text-center pt16 pb16" data-name="Buttons">
                            <a href="/contactus" class="mb8 mt8 flat flat flat btn btn-epsilon flat btn-lg"
                               data-original-title="" title="" aria-describedby="tooltip964010">SHOP NOW
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Banner End-->

        <!--Snippet Sale Image Start-->
        <template id="automobile_sale_image" name="Automobile Sale Image">
            <section class="pt64 pb64" id="ecommerce_block_3">
                <div class="container">
                    <div class="row">
                        <div class="col-12 col-md-4 col-lg-4 col-sm-6 mt8 mb8">
                            <div class="hover_effect">
                                <a href="#">
                                    <img class="img-fluid d-block mx-auto"
                                         src="/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-360x525.png"
                                         data-original-title="" title="" aria-describedby="tooltip867057"/>
                                </a>
                                <div class="col-12 img-content-2 mt-lg-5">
                                    <p style="text-align: center; ">
                                        <a href="#" class="btn btn-epsilon flat btn-lg" data-original-title="" title=""
                                           aria-describedby="tooltip373208">HELMETS
                                        </a>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-md-8 col-lg-8 col-sm-6">
                            <div class="row">
                                <div class="col-12 co-md-6 col-lg-6 col-sm-6 mt8 mb8">
                                    <div class="hover_effect">
                                        <a href="#">
                                            <img class="img-fluid d-block mx-auto"
                                                 src="/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-360x250.png"
                                                 data-original-title="" title="" aria-describedby="tooltip867057"/>
                                        </a>
                                        <div class="col-12 img-content-2 mt-lg-4">
                                            <p style="text-align: center; ">
                                                <a href="#" class="btn btn-epsilon flat btn-lg" data-original-title=""
                                                   title="" aria-describedby="tooltip256968">BODY&amp;nbsp; PARTS
                                                </a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 co-md-6 col-lg-6 col-sm-6 mt8 mb8">
                                    <div class="hover_effect">
                                        <a href="#">
                                            <img class="img-fluid d-block mx-auto"
                                                 src="/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-360x250.png"
                                                 data-original-title="" title="" aria-describedby="tooltip867057"/>
                                        </a>
                                        <div class="col-12 img-content-2 mt-lg-4">
                                            <p style="text-align: center; ">
                                                <a href="#" class="btn btn-epsilon flat btn-lg" data-original-title=""
                                                   title="" aria-describedby="tooltip330119">LIGHTS &amp;amp; LAMP
                                                </a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-12 col-lg-12 col-sm-12 mt-lg-3 mb16">
                                    <div class="hover_effect">
                                        <a href="#">
                                            <img class="img-fluid d-block mx-auto"
                                                 src="/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-750x250.png"
                                                 data-original-title="" title="" aria-describedby="tooltip867057"/>
                                        </a>
                                        <div class="col-12 img-content-4 text-center">
                                            <h5>
                                                <a href="#" class="btn btn-epsilon flat btn-lg">WHEELS &amp;amp; TIRES
                                                </a>
                                            </h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Sale Image End-->

        <!--Snippet Category Carousel Start-->
        <template id="automobile_category_carousel" name="Automobile Category Carousel">
            <section class="s_product_category_carousel js_get_objects mb8 s_headline_center bg-beta oe_custom_bg pt48"
                     data-loading="true"
                     data-template="ecommerce_snippets_product_category.product_category_list_template_header_d9"
                     data-object_name="product.public.category" data-objects_in_slide="5" id="new_category_item_d9"
                     style=""/>
        </template>
        <!--Snippet Category Carousel End-->

        <!--Snippet Product Carousel Start-->
        <template id="automobile_product_carousel" name="Automobile Product Carousel">
            <section
                    class="s_product_carousel oe_website_sale js_get_objects mb8 product_carousel pt40 s_headline_center"
                    data-loading="true" data-template="ecommerce_snippets_blocks.product_list_template_d8"
                    data-object_name="product.template" data-objects_in_slide="4" id="new_products_item_d8"
                    data-objects_in_row="2" data-objects_limit="15" style=""/>
        </template>
        <!--Snippet Product Carousel End-->

        <!--Snippet Promo Image Start-->
        <template id="automobile_promo_image" name="Automobile Promo Image">
            <section class="pt64 pb64 oe_img_bg oe_custom_bg"
                     style="background-image: url(&quot;/theme_automobile_ecommerce_ultimate_01/static/src/img/placeholder-1170.jpg&quot;);">
                <div class="container">
                    <div class="row justify-content-end">
                        <div class="col-12 col-sm-12 col-md-6 col-lg-8 right  text-center pt64 pb64">
                            <h2 class="s_letter_spacing_3">
                                <font style="color: rgb(255, 255, 255); font-size: 18px;"
                                      class="o_default_snippet_text">CUSTOM WHEELS
                                </font>
                            </h2>
                            <h2 class="s_letter_spacing_3">
                                <font style="color: rgb(255, 255, 255); font-size: 36px;"
                                      class="o_default_snippet_text">SYSTEMS NEW TECHNOLOGY
                                </font>
                            </h2>
                            <a href="#" class="text-decoration-none btn btn-epsilon flat btn-lg" data-original-title=""
                               title="" aria-describedby="tooltip250793">Shop Now
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Promo Image End-->

        <!--Snippet Blog Start-->
        <template id="automobile_blog" name="Automobile Blog">
            <section class="s_blog_carousel js_get_objects blog_carousel pt40 s_headline_center" data-loading="true"
                     data-template="snippet_blog_carousel_73lines.blog_list_template" data-object_name="blog.post"
                     style=""/>
        </template>
        <!--Snippet Blog End-->

        <!--Snippet Brand Carousel Start-->
        <template id="automobile_brand_carousel" name="Automobile Brand Carousel">
            <section class="s_brand_carousel mb8 js_get_objects pt40 s_headline_center" data-loading="true"
                     data-template="snippet_product_brand_carousel_73lines.brand_list_template_header"
                     data-object_name="product.brand" data-objects_in_slide="5" style=""/>
        </template>
        <!--Snippet Brand Carousel End-->

        <!--Snippet Features Start-->
        <template id="automobile_features" name="Automobile Features">
            <section class="bg-alpha oe_custom_bg pt16 pb16">
                <div class="container">
                    <div class="row">
                        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="row ">
                                <div class="col-12 col-sm-12 col-md-4 col-lg-4  media">
                                    <i class="fa fa-3x mr-3 mt-2 text-gamma pe-7s-headphones fa-500px"
                                       data-original-title="" title="" aria-describedby="tooltip334152" style=""/>
                                    <div class="media-body">
                                        <h3 class=" mt8">
                                            <font style="color: rgb(255, 255, 255); font-size: 18px;">24x7 Support
                                            </font>
                                        </h3>
                                    </div>
                                </div>
                                <div class="col-12 col-sm-12 col-md-4 col-lg-4  media">
                                    <i class="fa fa-3x fa-500px mr-3 mt-2 pe-7s-cash text-gamma" data-original-title=""
                                       title="" aria-describedby="tooltip423760" style=""/>
                                    <div class="media-body">
                                        <h3 class=" mt8">
                                            <font style="color: rgb(255, 255, 255); font-size: 18px;">Money Back
                                                Guarantee
                                            </font>
                                        </h3>
                                    </div>
                                </div>
                                <div class="col-12 col-sm-12 col-md-4 col-lg-4  media">
                                    <i class="fa fa-3x fa-500px mr-3 mt-2 pe-7s-car text-gamma" data-original-title=""
                                       title="" aria-describedby="tooltip468230" style=""/>
                                    <div class="media-body">
                                        <h3 class="mt8 ">
                                            <font style="color: rgb(255, 255, 255); font-size: 18px;">Free Shipping
                                            </font>
                                        </h3>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Features End-->

        <!--Snippet Footer Start-->
        <template id="automobile_footer" name="Automobile Footer">
            <section id="footer" class="oe_structure oe_structure_solo" t-ignore="true" t-if="not no_footer"
                     style="border-top:none;">
                <div class="pt64 pb32" id="footer_3">
                    <div class="container">
                        <div class="row">
                            <div class="col-12 col-sm-12 col-md-6 col-lg-4 mb32">
                                <h5>
                                    <font style="color: rgb(255, 255, 255);">CONTACT</font>
                                </h5>
                                <p class="mb32 mt24 o_default_snippet_text">
                                    <font class="text-delta" style="">Lorem ipsum dolor sit amet, consectetur
                                        adipisicing elit, sed do eiusmod tempor incididunt ut.&amp;nbsp;
                                    </font>
                                </p>
                                <div class="o_default_snippet_text media mb16">
                                    <i class="fa fa-home mr-3 mt-1 text-delta" data-original-title="" title=""
                                       aria-describedby="tooltip224421" style=""/>
                                    <div class="media-body">
                                        <font class="text-delta" style="">
                                            <span t-field="res_company.street"/>
                                            <span t-field="res_company.city"/>
                                            <span t-field="res_company.state_id.name"/>
                                        </font>
                                    </div>
                                </div>
                                <div class="o_default_snippet_text media mb16">
                                    <i class="fa fa-envelope-o mr-3 mt-1 text-delta" style=""/>
                                    <div class="media-body">
                                        <font class="text-delta" style="">
                                            <span t-field="res_company.email"/>
                                        </font>
                                    </div>
                                </div>
                                <div class="o_default_snippet_text media mb16">
                                    <i class="fa fa-phone mr-3 mt-1 text-delta" style=""/>
                                    <div class="media-body">
                                        <font class="text-delta" style="">
                                            <span t-field="res_company.phone"/>
                                        </font>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12 col-sm-12 col-md-6 col-lg-2 mb32">
                                <h5>
                                    <font style="color: rgb(255, 255, 255);">MY ACCOUNT</font>
                                </h5>
                                <ul class="list-unstyled mt24">
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">My Account</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Order History</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Wishlist</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Personal Info</font>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-12 col-sm-12 col-md-6 col-lg-2 mb32">
                                <h5>
                                    <font style="color: rgb(255, 255, 255);">INFORMATION</font>
                                </h5>
                                <ul class="list-unstyled mt24">
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">AbouUs</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Delivery Information</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Privacy Policy</font>
                                        </a>
                                    </li>
                                    <li class="mb8">
                                        <a href="#" target="_blank" class="o_default_snippet_text">
                                            <font style="" class="text-delta">Terms &amp;amp; Condition</font>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-12 col-sm-12 col-md-6 col-lg-4 mb32">
                                <h5 class="mb24">
                                    <font style="color: rgb(255, 255, 255);">OUR NEWSLETTER</font>
                                </h5>
                                <p class="o_default_snippet_text">
                                    <font class="text-delta" style="">Lorem Ipsum is simply dummy text of the
                                        printing and typesetting industry. Lorem Ipsum has been the industry's
                                        standard dummy.&amp;nbsp;
                                    </font>
                                </p>
                                <div class="input-group js_subscribe hidden d-block" data-list-id="1"
                                     data-subscribe="off">
                                    <input type="email" name="email"
                                           class="js_subscribe_email form-control w-100 mb8"
                                           placeholder="your email..."/>
                                    <div class="input-group-btn">
                                        <a href="#"
                                           class="btn btn-primary js_subscribe_btn o_default_snippet_text w-100">
                                            Subscribe
                                        </a>
                                        <a href="#"
                                           class="btn btn-success js_subscribed_btn hidden o_default_snippet_text w-100 d-none"
                                           disabled="disabled">Thanks
                                        </a>
                                    </div>
                                    <div class="alert alert-success hidden o_default_snippet_text">Thanks for your
                                        subscription!
                                    </div>
                                </div>
                                <div class="icon_bg mt-2">
                                    <a t-att-href="res_company.social_facebook">
                                        <i class="fa fa-facebook  text-delta" data-original-title="" title=""
                                           aria-describedby="tooltip816516" style="background-color: unset;"/>
                                    </a>
                                    <a t-att-href="res_company.social_twitter">
                                        <i class="fa fa-twitter text-delta" style="background-color: unset;"
                                           data-original-title="" title="" aria-describedby="tooltip709641"/>
                                    </a>
                                    <a t-att-href="res_company.social_instagram">
                                        <i class="fa fa-instagram  text-delta" style="background-color: unset;"
                                           data-original-title="" title="" aria-describedby="tooltip118072"/>
                                    </a>
                                    <a href="#">
                                        <i class="fa fa-google-plus  text-delta" data-original-title="" title=""
                                           aria-describedby="tooltip245883" style="background-color: unset;"/>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Footer End-->

        <!--Snippets Assets-->
        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Automobile Ecommerce Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_banner"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_sale_image"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_category_carousel"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_product_carousel"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_promo_image"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_blog"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_brand_carousel"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_features"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_automobile_ecommerce_ultimate_01.automobile_footer"
                           t-thumbnail="/theme_automobile_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>