        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Security Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_repair_ultimate_01.repair_banner"
                           t-thumbnail="/theme_repair_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_repair_ultimate_01.repair_feature"
                           t-thumbnail="/theme_repair_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_repair_ultimate_01.repair_service"
                           t-thumbnail="/theme_repair_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
