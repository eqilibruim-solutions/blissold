<odoo>
    <data>

        <!--Snippet Mid Header Start-->
        <template id="cap_store_mid_header" name="Cap Store Mid Header">
            <section class="bg-light pb-1 pb-1 bg-black oe_custom_bg" id="mid-header-top-3" style="">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-12 oe_structure mt-1 mb-1 col-lg-8 col-md-12 col-sm-12 text-center text-lg-left text-md-center"
                             id="mid-header-top-left">
                            <li class="list-inline-item">
                                <div class="oe_structure oe_empty">
                                    <span id="phone-header" class="col ml8 mr8">
                                        <i class="fa fa-phone mr-2"/>
                                        <span style="font-size: 13px;" t-field="res_company.phone"/>
                                    </span>
                                </div>
                            </li>
                            <li class="list-inline-item">
                                <div class="oe_structure oe_empty">
                                    <span class="col ml8 mr8 text-center" id="email-header" style="">
                                        <i class="fa fa-envelope mr-2"/>
                                        <span t-field="res_company.email"/>
                                    </span>
                                </div>
                            </li>
                        </div>
                        <div class="col-12 oe_structure col-lg-4 col-md-12 col-sm-12 my-1 text-center text-lg-right text-md-center"
                             id="mid-header-top-right">
                            <li class="list-inline-item">
                                <div class="oe_structure oe_empty">
                                    <span class="mr-2">
                                        <i class="fa fa-location-arrow"/>
                                        <a href="#" style="color: rgb(0, 0, 0);" class="o_default_snippet_text">
                                            <font class="text-gamma" style="">Store Location</font>
                                        </a>
                                    </span>
                                </div>
                            </li>
                            <li class="list-inline-item">
                                <div class="oe_structure oe_empty">
                                    <span>
                                        <i class="fa fa-first-order"/>
                                        <a href="#" style="color: rgb(0, 0, 0);" class="o_default_snippet_text">
                                            <font class="text-gamma" style="">Track Order</font>
                                        </a>
                                    </span>
                                </div>
                            </li>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Mid Header End-->


        <!--Snippet Banner Start-->
        <template id="cap_store_banner" name="Cap Store Banner">
            <section
                    class="s_cover parallax align-content-center align-items-center d-flex full-height s_parallax_is_fixed s_parallax_no_overflow_hidden"
                    data-scroll-background-ratio="1" style="background-image: none;">
                <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                      style="background-image: url(&quot;/theme_cap_store_ecommerce_ultimate_01/static/src/img/placeholder-image-1170.jpg&quot;); background-position: 50% 0px;"/>
                <div class="container">
                    <div class="row s_nb_column_fixed">
                        <div class="col-lg-12 s_title" data-name="Title">
                            <h1 class="text-left" style="font-size: 62px;">
                                <font class="text-alpha" style="font-size: 48px;">Mid Season Sale</font>
                            </h1>
                        </div>
                        <div class="col-lg-12 s_text pt16 pb16" data-name="Text">
                            <p class="lead text-left o_default_snippet_text" style="text-align: center;">Write one or
                                two paragraphs describing your product, services or a specific feature.
                                <br/>
                                To be successful your content needs to be useful to your readers.
                            </p>
                        </div>
                        <div class="col-lg-12 text-left pt16 pb16" data-name="Buttons">
                            <a href="/aboutus" class="mt8 mb8 btn btn-beta flat" data-original-title="" title=""
                               aria-describedby="tooltip131972">
                                <font class="text-epsilon" style="">Discover store</font>
                            </a>
                            <a href="/contactus" class="mt8 mb8 btn btn-epsilon flat" data-original-title="" title=""
                               aria-describedby="tooltip312291">
                                <font style="color: rgb(0, 0, 0);">learn more</font>
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Banner End-->

        <!--Snippet Image Block Start-->
        <template id="cap_store_image_block" name="Cap Store Image Block">
            <section class="sni-block pt64 pb64">
                <div class="container">
                    <div class="row">
                        <div class="col-12 co-md-6 col-lg-6 col-sm-12">
                            <div class="row">
                                <div class="col-12 co-md-12 col-lg-12 col-sm-6">
                                    <img class="img-fluid d-block mx-auto mt16 mb16"
                                         src="/theme_cap_store_ecommerce_ultimate_01/static/src/img/placeholder-562x293.png"/>
                                </div>
                                <div class="col-12 co-md-12 col-lg-12 col-sm-6">
                                    <img class="img-fluid d-block mx-auto mt16 mb16"
                                         src="/theme_cap_store_ecommerce_ultimate_01/static/src/img/placeholder-562x293.png"/>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 co-md-6 col-lg-6 col-sm-12">
                            <img class="img-fluid d-block mx-auto mt16 mb16"
                                 src="/theme_cap_store_ecommerce_ultimate_01/static/src/img/placeholder-560x632.png"/>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Image Block End-->

        <!--Snippet Newsletter Start-->
        <template id="cap_store_newsletter" name="Cap Store Newsletter">
            <section style="background-color:#f3f3f3;">
                <div class="container">
                    <div class="row">
                        <div class="col-12 col-lg-6 col-md-6 col-sm-6 mh100 pl-5">
                            <div class="oe_structure oe_empty">
                                <section class="pt64">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-12 col-sm-12 col-md-12 col-lg-12  media">
                                                <div class="media-body">
                                                    <h3>
                                                        <font class="text-alpha" style="font-size: 30px;">Winter sale
                                                        </font>
                                                        <br/>
                                                    </h3>
                                                    <p class="text-beta" style="">
                                                        <font style="font-size: 18px;" class="text-alpha">Sign Up To
                                                            Enjoy Free Shipping On Your First Order
                                                        </font>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                            </div>
                        </div>
                        <div class="col-12 col-lg-6 col-md-6 col-sm-6 mh100 pr-5">
                            <div class="oe_structure oe_empty">
                                <section class="pt64 pb32" id="footer_7">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-12 col-sm-12 col-md-12 col-lg-11 offset-lg-1 mb48">
                                                <div class="input-group js_subscribe hidden" data-list-id="1"
                                                     data-subscribe="off">
                                                    <input type="email" name="email"
                                                           class="js_subscribe_email form-control br-left"
                                                           placeholder="your email..."
                                                           style="height: 55px !important;"/>
                                                    <span class="input-group-btn">
                                                        <a href="#"
                                                           class="br-right px-3 py-3 js_subscribe_btn btn btn-beta"
                                                           style="border-radius: 0 !important;" data-original-title=""
                                                           title="" aria-describedby="tooltip4742">
                                                            <font class="text-epsilon" style="">Subscribe</font>
                                                        </a>
                                                        <a href="#"
                                                           class="btn br-right px-3 py-3 btn-success js_subscribed_btn hidden o_default_snippet_text d-none"
                                                           disabled="disabled" style="border-radius: 0 !important;">
                                                            Thanks
                                                        </a>
                                                    </span>
                                                    <div class="alert alert-success hidden o_default_snippet_text d-none">
                                                        Thanks for your subscription!
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Newsletter End-->

        <!--Snippet Product Carousel One Start-->
        <template id="cap_store_product_carousel_one" name="Cap Store Product Carousel One">
            <section class="pt64 s_product_carousel oe_website_sale js_get_objects product_carousel s_headline_style_9"
                     data-loading="true" data-template="ecommerce_snippets_blocks.product_list_template_d20"
                     data-object_name="product.template" data-objects_in_slide="3" id="new_products_item_d20"
                     data-objects_limit="6" data-objects_in_row="2" style=""/>
        </template>
        <!--Snippet Product Carousel One End-->

        <!--Snippet Product Carousel Two Start-->
        <template id="cap_store_product_carousel_two" name="Cap Store Product Carousel Two">
            <section class="s_product_carousel oe_website_sale js_get_objects product_carousel s_headline_style_9"
                     data-loading="true" data-template="ecommerce_snippets_blocks.product_list_template_d20"
                     data-object_name="product.template" data-objects_in_slide="4" id="new_products_item_d20" style=""
                     data-objects_limit="8" data-objects_in_row="2"/>
        </template>
        <!--Snippet Product Carousel Two End-->

        <!--Snippet Brand Carousel Start-->
        <template id="cap_store_brand_carousel" name="Cap Store Brand Carousel">
            <section class="s_brand_carousel js_get_objects headline_hidden" data-loading="true"
                     data-template="ecommerce_snippets_product_brand.brand_list_template_header_d4"
                     data-object_name="product.brand" data-objects_in_slide="5" id="new_brand_item_d4"/>
        </template>
        <!--Snippet Brand Carousel End-->

        <!--Snippet Footer Start-->
        <template id="cap_store_footer" name="Cap Store Footer">
            <section id="footer" class="oe_structure oe_structure_solo" t-ignore="true" t-if="not no_footer"
                     style="border-top:none;">
                <div class="pt64 pb32" id="footer_15">
                    <div class="container">
                        <div class="row">
                            <div class="col-12 col-sm-12 col-md-3 col-lg-3">
                                <h5 class="pb16">
                                    <font class="text-epsilon" style="">About</font>
                                </h5>
                                <ul class="list-unstyled">
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Our Shop</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Contact Us</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Blog</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">FAQs</font>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-12 col-sm-12 col-md-3 col-lg-3">
                                <h5 class="pb16">
                                    <font class="text-epsilon" style="">Products</font>
                                </h5>
                                <ul class="list-unstyled">
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Knitted</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Fitted</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Snapback</font>
                                    </li>
                                    <li>
                                        <font class="text-delta s_line_height_2" style="">Stretch</font>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-12 col-sm-12 col-md-3 col-lg-3">
                                <h5 class="pb16">
                                    <font class="text-epsilon" style="">Social</font>
                                </h5>
                                <ul class="list-unstyled">
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Facebook</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Twitter</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Instagram</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Pinterest</font>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-12 col-sm-12 col-md-3 col-lg-3">
                                <h5 class="pb16">
                                    <font class="text-epsilon" style="">Customer Care</font>
                                </h5>
                                <ul class="list-unstyled">
                                    <li class="o_default_snippet_text">
                                        <font class="s_line_height_2 text-delta" style="">Services &amp;amp; Help</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Stock</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Special Offers</font>
                                    </li>
                                    <li class="o_default_snippet_text">
                                        <font class="text-delta s_line_height_2" style="">Guide</font>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Footer End-->

        <!--Snippets Assets-->
        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Cap Store Ecommerce Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_mid_header"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_banner"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_image_block"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_newsletter"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_product_carousel_one"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_product_carousel_two"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_brand_carousel"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_cap_store_ecommerce_ultimate_01.cap_store_footer"
                           t-thumbnail="/theme_cap_store_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>