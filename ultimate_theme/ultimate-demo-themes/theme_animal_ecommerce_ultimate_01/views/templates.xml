                        <div class="col-12 col-md-6 col-lg-6 col-sm-6 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto" src="/theme_animal_ecommerce_ultimate_01/static/src/img/placeholder-image-600.jpg"
                                     data-original-title="" title="" aria-describedby="tooltip867057"/>
                                <div class="col-12 img-content px-5">
                                    <h2 style="text-align: center; ">
                                        <font style="color: rgb(255, 255, 255);">MAKE THEIR DAY</font>
                                    </h2>
                                    <p style="text-align: center; ">
                                        <font style="color: rgb(255, 255, 255); font-size: 18px;">Up to</font>
                                        <font style="color: rgb(255, 255, 255); font-size: 18px;" class="bg-beta">
                                            <b>30% OFF</b>
                                        </font>
                                        <font style="color: rgb(255, 255, 255); font-size: 18px;">Dog Treats</font>
                                    </p>
