                                <div class="col-12 col-sm-12 col-md-4 col-lg-4 text-center">
                                    <div class="bg-100 py-4">
                                        <i class="fa fa-3x fa-calculator"/>
                                        <h3 class="pb8 pt8">
                                            <font style="font-size: 18px;">Money Return</font>
                                        </h3>
                                        <p class="mb0">Back guarantee under 5 days</p>
                                    </div>
                                </div>
