        <!--Snippet Brand Logo Start-->
        <template id="animal_brand_logo" name="Animal Brand Logo">
            <section class="pt64 pb64">
                <div class="container">
                    <div class="row">
                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
                        </div>
                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
                        </div>
                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
