                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
                        </div>
                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
                        </div>
                        <div class="col-12 col-sm-2 col-md-2 col-lg-2 text-center">
                            <img src="/theme_animal_ecommerce_ultimate_01/static/src/img/brand-logo.png"
                                 class="img-fluid mb8 mt8" alt="Brand Logo" title="Brand Logo"/>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Brand Logo End-->

        <!--Snippets Assets-->
        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Animal Ecommerce Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_animal_ecommerce_ultimate_01.animal_features"
                           t-thumbnail="/theme_animal_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_animal_ecommerce_ultimate_01.animal_image_block_one"
                           t-thumbnail="/theme_animal_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_animal_ecommerce_ultimate_01.animal_image_block_two"
                           t-thumbnail="/theme_animal_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_animal_ecommerce_ultimate_01.animal_brand_logo"
                           t-thumbnail="/theme_animal_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>