<odoo>
    <data>

        <!--Snippet Promo Image Start-->
        <template id="wedding_promo_image" name="Wedding Promo Image">
            <section class="pt64 pb64" id="ecommerce_block_2">
                <div class="container">
                    <div class="row">
                        <div class="col-12 co-md-4 col-lg-4 col-sm-4 mt8 mb8 smooth-image">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto"
                                     src="/theme_wedding_ecommerce_ultimate_01/static/src/img/placeholder-720x948.png"
                                     data-original-title="" title="" aria-describedby="tooltip668560"/>
                                <div class="img-content-1 col-12">
                                    <h3 class="mb16">
                                        <i class="o_default_snippet_text">
                                            <b>
                                                <font class="text-alpha" style="">THE</font>
                                                <br/>
                                                <font class="text-alpha" style="">NEW</font>
                                                <br/>
                                                <font class="text-alpha" style="">LONG</font>
                                                <br/>
                                                <font class="text-alpha" style="">DRESSES&amp;nbsp;</font>
                                            </b>
                                        </i>
                                    </h3>
                                    <p class="o_default_snippet_text">
                                        <font style="" class="text-epsilon">MIX OF TRADITION</font>
                                        <br/>
                                        <font style="" class="text-epsilon">AND MODERNITY&amp;nbsp;</font>
                                    </p>
                                    <div class="pt16 pb16" data-name="Buttons">
                                        <a href="#" class="mt8 mb8 btn btn-epsilon" data-original-title="" title=""
                                           aria-describedby="tooltip345831">
                                            <b>
                                                <font>View More</font>
                                            </b>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 co-md-4 col-lg-4 col-sm-4 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto"
                                     src="/theme_wedding_ecommerce_ultimate_01/static/src/img/placeholder-720x948.png"
                                     data-original-title="" title="" aria-describedby="tooltip563848"/>
                                <div class="img-content-2 text-center col-12">
                                    <h3 class="o_default_snippet_text">
                                        <i class="o_default_snippet_text">
                                            <font style="color: rgb(255, 255, 255);">It's all in the</font>
                                        </i>
                                    </h3>
                                    <h2 class="o_default_snippet_text">
                                        <i class="o_default_snippet_text">
                                            <font style="" class="text-alpha">
                                                <b>DETAILS</b>
                                            </font>
                                        </i>
                                    </h2>
                                    <p class="o_default_snippet_text">
                                        <font style="color: rgb(247, 247, 247);">MAKE YOUR BIG DAY SHINE</font>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 co-md-4 col-lg-4 col-sm-4 mt8 mb8">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto"
                                     src="/theme_wedding_ecommerce_ultimate_01/static/src/img/placeholder-720x948.png"
                                     data-original-title="" title="" aria-describedby="tooltip576559"/>
                                <div class="img-content-1 text-center col-12">
                                    <h6>
                                        <i class="o_default_snippet_text">
                                            <font class="text-alpha" style="">BRIDLE</font>
                                        </i>
                                    </h6>
                                    <h3>
                                        <i>
                                            <font class="text-alpha" style="">
                                                <b>LINGERIE</b>
                                            </font>
                                        </i>
                                    </h3>
                                    <p class="o_default_snippet_text">
                                        <i>
                                            <font style="font-size: 16px;" class="text-alpha">Wedding night</font>
                                            <br/>
                                            <font style="font-size: 16px;" class="text-alpha">lingerie collection &amp;nbsp;</font>
                                        </i>
                                    </p>
                                    <div class="text-center pt16 pb16" data-name="Buttons">
                                        <a href="#" class="mt8 mb8 btn btn-epsilon" data-original-title="" title="">
                                            <b>View More</b>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 co-md-12 col-lg-12 col-sm-12 mt8 mb8 image_block_4">
                            <div class="hover_effect">
                                <img class="img-fluid d-block mx-auto"
                                     src="/theme_wedding_ecommerce_ultimate_01/static/src/img/placeholder-1140x174.png"/>
                                <div class="img-content-1 col-12 text-center">
                                    <h3 class="o_default_snippet_text">
                                        <i class="o_default_snippet_text">
                                            <font class="text-alpha" style="">DEALS + PROMOS&amp;nbsp;</font>
                                        </i>
                                    </h3>
                                    <p class="o_default_snippet_text">
                                        <font class="text-alpha" style="">CLASSY GOWN DRESSES</font>
                                    </p>
                                    <div class="text-center" data-name="Buttons">
                                        <a href="#" class="mt8 mb8 btn btn-epsilon" data-original-title="" title=""
                                           aria-describedby="tooltip109242">
                                            <b>View More</b>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Promo Image End-->

        <!--Snippet Sale Image Start-->
        <template id="wedding_sale_image" name="Wedding Sale Image">
            <section class="pt64 pb64 oe_img_bg oe_custom_bg"
                     style="background-image: url(&quot;/theme_wedding_ecommerce_ultimate_01/static/src/img/placeholder-image-1170.jpg&quot;);">
                <div class="container ">
                    <div class="row">
                        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
                            <h2 class="font-weight-bold pt24 pb24" style="text-align: left; ">
                                <span style="font-weight: normal;">
                                    <i>
                                        <font style="font-size: 36px;" class="o_default_snippet_text text-gamma">Let Us
                                            Make Your Wedding Flawless
                                        </font>
                                    </i>
                                </span>
                            </h2>
                            <p class="lead text-white o_default_snippet_text">
                                <font class="text-delta" style="">Write one or two paragraphs describing your product,
                                    services or a specific feature.&amp;nbsp;
                                </font>
                                <br/>
                                <font class="text-delta" style="">&amp;nbsp;To be successful your content needs to be
                                    useful to your readers.&amp;nbsp;
                                </font>
                            </p>
                            <a href="#" class="rounded-0 mt16 mb16 btn btn-beta btn-lg" data-original-title="" title=""
                               aria-describedby="tooltip430823">SHOP FOR YOU
                            </a>
                        </div>
                    </div>
                </div>
            </section>

        </template>
        <!--Snippet Sale Image End-->

        <!--Snippets Assets Start-->
        <template id="demo_snippets" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_banner']" position="before">
                <div id="snippet_demo_snippets" class="o_panel">
                    <div class="o_panel_header">
                        <i class="fa fa-th-large"/>
                        Wedding Ecommerce Ultimate 01
                    </div>
                    <div class="o_panel_body">
                        <t t-snippet="theme_wedding_ecommerce_ultimate_01.wedding_promo_image"
                           t-thumbnail="/theme_wedding_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                        <t t-snippet="theme_wedding_ecommerce_ultimate_01.wedding_sale_image"
                           t-thumbnail="/theme_wedding_ecommerce_ultimate_01/static/src/img/snippet-icons/demo-snippet-icon.png"/>
                    </div>
                </div>
            </xpath>
        </template>
        <!--Snippets Assets End-->
    </data>
</odoo>