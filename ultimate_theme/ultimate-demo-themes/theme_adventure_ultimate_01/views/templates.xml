                    data-scroll-background-ratio="1" style="background-image: none; height: 100vh !important;">
                <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                      style="background-image: url(&quot;/theme_adventure_ultimate_01/static/src/img/placeholder-1170.jpg&quot;); background-position: 50% 0px;"/>
                <div class="container">
                    <div class="row s_nb_column_fixed">
                        <div class="col-lg-12 s_title" data-name="Title">
                            <h1 class="s_title_thin text-left" style="font-size: 62px;">Amazing Adventure</h1>
                            <h1 class="s_title_thin text-left" style="font-size: 62px;">Awaits</h1>
                        </div>
                        <div class="col-lg-12 s_text pt16 pb16" data-name="Text">
                            <p class="lead text-left o_default_snippet_text" style="text-align: center;">Write one or
                                two paragraphs describing your product, services or a specific feature.
                                <br/>
                                To be successful your content needs to be useful to your readers.
                            </p>
                        </div>
                        <div class="col-lg-12 s_btn text-left pt16 pb16" data-name="Buttons">
                            <a href="/contactus" class="rounded-circle btn btn-primary rounded-circle btn-lg"
                               data-original-title="" title="" aria-describedby="tooltip280957">
                                <font style="color: rgb(255, 255, 255);">View More Details</font>
                            </a>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <!--Snippet Banner End-->

